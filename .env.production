# Production Environment Variables
# Replace these values with your actual production values

# Base URL for the application
VITE_APP_BASE_URL=https://memory.veeda.space

#---------------------------------
# Supabase Configuration
#---------------------------------
VITE_SUPABASE_URL=your-production-supabase-url
VITE_SUPABASE_KEY=your-production-supabase-key

#---------------------------------
# AI Provider Configuration
#---------------------------------
# Default AI provider (openai or gemini)
VITE_AI_DEFAULT_PROVIDER=openai

# OpenAI Configuration
VITE_OPENAI_API_KEY=your-production-openai-api-key
VITE_OPENAI_MODEL=gpt-4o-mini
VITE_OPENAI_BASE_URL=https://api.openai.com/v1

# Google Gemini Configuration
VITE_GEMINI_API_KEY=your-production-gemini-api-key
VITE_GEMINI_MODEL=gemini-2.0-flash

#---------------------------------
# Feature Flags
#---------------------------------
# AI features (true/false)
VITE_ENABLE_AI_TITLE_GENERATION=true
VITE_ENABLE_AI_TAG_SUGGESTIONS=true
VITE_ENABLE_AI_CONTENT_SUMMARY=false
VITE_ENABLE_TEXT_FORMATTING=false 